<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.hotel.dao.IRoundTypeDao">
    <select id="getAllRoundType" resultType="RoundType" >
		select * from roundtype
	</select>

	<select id="getRoundType" resultType="RoundType" >
		select * from roundtype where rotid=#{rotid}
	</select>




</mapper>